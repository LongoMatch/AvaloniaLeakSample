<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:sample="clr-namespace:AvaloniaLeakSample"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AvaloniaLeakSample.View2">
    
    <Grid RowDefinitions="Auto, Auto, Auto, *"
          Background="{DynamicResource ScreenBaseBrush}">
        <Button Grid.Row="0" Click="Button_Click">BACK</Button>
        <sample:NestedControl1 Grid.Row="1" Margin="10,0"
                               Name="NestedControl"/>
        <!-- <Button Height="30" Width="100" Name="OtherButton"/> -->
    </Grid>
    
</UserControl>
